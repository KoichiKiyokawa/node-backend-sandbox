{
  "info": {
    "title": "My API",
    "version": "1.0.0"
  },
  "openapi": "3.1.0",
  "components": {
    "schemas": {},
    "parameters": {}
  },
  "paths": {
    "/users": {
      "get": {
        "responses": {
          "200": {
            "description": "List of users",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "id": {
                        "type": "string"
                      },
                      "name": {
                        "type": "string"
                      },
                      "email": {
                        "type": "string",
                        "format": "email"
                      },
                      "birthday": {
                        "type": "string"
                      }
                    },
                    "required": [
                      "id",
                      "name",
                      "email",
                      "birthday"
                    ]
                  }
                }
              }
            }
          },
          "500": {
            "description": "Internal Server Error",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "code": {
                      "type": "number"
                    },
                    "message": {
                      "type": "string"
                    }
                  },
                  "required": [
                    "code",
                    "message"
                  ]
                }
              }
            }
          }
        }
      }
    }
  }
}
